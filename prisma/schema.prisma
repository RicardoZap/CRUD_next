generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int      @id @default(autoincrement())
  name          String
  ap_paterno    String
  ap_materno    String
  birthday_date DateTime
  cellphone     String
  email         String
}

model Enterprise {
  id        Int    @id @default(autoincrement())
  name      String
  address   String
  cellphone String
  email     String
}
